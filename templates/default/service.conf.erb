# Generated by Chef for <%= node['fqdn'] %>
# Local modifications will be overridden
#
# <%= @name.upcase %>

check <%= @check_type.to_s %> <%= @name %>
<% if @check_with %>
  with <%= @check_with %>
<% end -%>
<% if @start_program %>
start program = "<%= @start_program %>"
  <% if @start_with %>
  with <%= @start_with %>
  <% end -%>
<% end -%>
<% if @start_program %>
stop program = "<%= @stop_program %>"
<% if @program_uid %>
  as uid <%= @program_uid %>
  <% if @program_gid %>
  and gid <%= @program_gid %>
  <% end -%>
<% elsif @program_gid %>
  as gid <%= @program_gid %>
<% end -%>
<% end -%>
<% if @group %>
group <%= @group %>
<% end -%>
<% Array(@conditions).each do |condition| %>
<%= condition %>
<% end -%>
